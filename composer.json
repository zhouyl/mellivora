{
    "name": "mellivora/mellivora",
    "type": "library",
    "description": "Mellivora PHP Framework",
    "keywords": ["framework", "micro", "slim", "laravel", "symfony", "mellivora", "claw"],
    "homepage": "https://github.com/zhouyl/mellivora",
    "license": "MIT",
    "authors": [
        {
            "name": "zhouyl",
            "email": "81438567@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "slim/slim": "^3.8",
        "doctrine/inflector": "^1.1",
        "zhouyl/carbon": "~1.23",
        "symfony/cache": "~3.3",
        "symfony/console": "~3.3",
        "symfony/debug": "~3.3",
        "symfony/http-foundation": "~3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2"
    },
    "autoload": {
        "psr-4": {
            "Mellivora\\": "Mellivora"
        },
        "files": [
            "helpers.php"
        ]
    },
    "scripts": {
        "phpcs": "./vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
    }
}
